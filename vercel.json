{
    "version": 2,
    "public": true,
    "buildCommand": "yarn install --ignore-engines && yarn build",
    "outputDirectory": "dist",
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/",
            "status": 200
        },
        {
            "src": "/(.+)",
            "dest": "/$1"
        }
    ],
    "github": {
        "silent": true
    }
} 
} 